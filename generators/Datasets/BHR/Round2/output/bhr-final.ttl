        <http://steinheim-institut.de:50580/cgi-bin/bhr?id=2067> ;
    skos:prefLabel "Carlebach, Emanuel" .

<http://data.judaicalink.org/data/bhr/Carlebach_Ephraim> a foaf:Person ;
    gndo:gndIdentifier "134282620" ;
